{
    "name": "divspace/responder",
    "description": "A Laravel 4 wrapper for Fractal to handle API responses",
    "keywords": [
        "laravel",
        "league",
        "fractal",
        "json",
        "rest",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kyle Anderson",
            "email": "kyle@divspace.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "illuminate/pagination": "4.2.*",
        "league/fractal": "0.11.*"
    },
    "provide": {
        "league/fractal": "0.11.*"
    },
    "autoload": {
        "psr-0": {
            "Divspace\\Responder\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
